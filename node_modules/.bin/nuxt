#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*) basedir=`cygpath -w "$basedir"`;;
esac

if [ -z "$NODE_PATH" ]; then
  export NODE_PATH="/mnt/d/website-opd-frontend/node_modules/.pnpm/nuxt@3.17.5_@parcel+watcher@2.5.1_@types+node@24.0.0_db0@0.3.2_ioredis@5.6.1_magicast@0.3.5_r_y34hyu6rc322wxzkyizr6seouu/node_modules/nuxt/bin/node_modules:/mnt/d/website-opd-frontend/node_modules/.pnpm/nuxt@3.17.5_@parcel+watcher@2.5.1_@types+node@24.0.0_db0@0.3.2_ioredis@5.6.1_magicast@0.3.5_r_y34hyu6rc322wxzkyizr6seouu/node_modules/nuxt/node_modules:/mnt/d/website-opd-frontend/node_modules/.pnpm/nuxt@3.17.5_@parcel+watcher@2.5.1_@types+node@24.0.0_db0@0.3.2_ioredis@5.6.1_magicast@0.3.5_r_y34hyu6rc322wxzkyizr6seouu/node_modules:/mnt/d/website-opd-frontend/node_modules/.pnpm/node_modules"
else
  export NODE_PATH="/mnt/d/website-opd-frontend/node_modules/.pnpm/nuxt@3.17.5_@parcel+watcher@2.5.1_@types+node@24.0.0_db0@0.3.2_ioredis@5.6.1_magicast@0.3.5_r_y34hyu6rc322wxzkyizr6seouu/node_modules/nuxt/bin/node_modules:/mnt/d/website-opd-frontend/node_modules/.pnpm/nuxt@3.17.5_@parcel+watcher@2.5.1_@types+node@24.0.0_db0@0.3.2_ioredis@5.6.1_magicast@0.3.5_r_y34hyu6rc322wxzkyizr6seouu/node_modules/nuxt/node_modules:/mnt/d/website-opd-frontend/node_modules/.pnpm/nuxt@3.17.5_@parcel+watcher@2.5.1_@types+node@24.0.0_db0@0.3.2_ioredis@5.6.1_magicast@0.3.5_r_y34hyu6rc322wxzkyizr6seouu/node_modules:/mnt/d/website-opd-frontend/node_modules/.pnpm/node_modules:$NODE_PATH"
fi
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../nuxt/bin/nuxt.mjs" "$@"
else
  exec node  "$basedir/../nuxt/bin/nuxt.mjs" "$@"
fi
