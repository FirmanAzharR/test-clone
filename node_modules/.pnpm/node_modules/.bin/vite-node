#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*) basedir=`cygpath -w "$basedir"`;;
esac

if [ -z "$NODE_PATH" ]; then
  export NODE_PATH="/mnt/d/website-opd-frontend/node_modules/.pnpm/vite-node@3.2.3_@types+node@24.0.0_jiti@2.4.2_sass-embedded@1.89.2_terser@5.42.0_yaml@2.8.0/node_modules/vite-node/node_modules:/mnt/d/website-opd-frontend/node_modules/.pnpm/vite-node@3.2.3_@types+node@24.0.0_jiti@2.4.2_sass-embedded@1.89.2_terser@5.42.0_yaml@2.8.0/node_modules:/mnt/d/website-opd-frontend/node_modules/.pnpm/node_modules"
else
  export NODE_PATH="/mnt/d/website-opd-frontend/node_modules/.pnpm/vite-node@3.2.3_@types+node@24.0.0_jiti@2.4.2_sass-embedded@1.89.2_terser@5.42.0_yaml@2.8.0/node_modules/vite-node/node_modules:/mnt/d/website-opd-frontend/node_modules/.pnpm/vite-node@3.2.3_@types+node@24.0.0_jiti@2.4.2_sass-embedded@1.89.2_terser@5.42.0_yaml@2.8.0/node_modules:/mnt/d/website-opd-frontend/node_modules/.pnpm/node_modules:$NODE_PATH"
fi
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../vite-node/vite-node.mjs" "$@"
else
  exec node  "$basedir/../vite-node/vite-node.mjs" "$@"
fi
