#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*) basedir=`cygpath -w "$basedir"`;;
esac

if [ -z "$NODE_PATH" ]; then
  export NODE_PATH="/mnt/d/website-opd-frontend/node_modules/.pnpm/@nuxt+devtools@2.5.0_vite@6.3.5_@types+node@24.0.0_jiti@2.4.2_sass-embedded@1.89.2_terser@5.4_gn7y4dzfmpiwkyxthv3hu732w4/node_modules/@nuxt/devtools/node_modules:/mnt/d/website-opd-frontend/node_modules/.pnpm/@nuxt+devtools@2.5.0_vite@6.3.5_@types+node@24.0.0_jiti@2.4.2_sass-embedded@1.89.2_terser@5.4_gn7y4dzfmpiwkyxthv3hu732w4/node_modules/@nuxt/node_modules:/mnt/d/website-opd-frontend/node_modules/.pnpm/@nuxt+devtools@2.5.0_vite@6.3.5_@types+node@24.0.0_jiti@2.4.2_sass-embedded@1.89.2_terser@5.4_gn7y4dzfmpiwkyxthv3hu732w4/node_modules:/mnt/d/website-opd-frontend/node_modules/.pnpm/node_modules"
else
  export NODE_PATH="/mnt/d/website-opd-frontend/node_modules/.pnpm/@nuxt+devtools@2.5.0_vite@6.3.5_@types+node@24.0.0_jiti@2.4.2_sass-embedded@1.89.2_terser@5.4_gn7y4dzfmpiwkyxthv3hu732w4/node_modules/@nuxt/devtools/node_modules:/mnt/d/website-opd-frontend/node_modules/.pnpm/@nuxt+devtools@2.5.0_vite@6.3.5_@types+node@24.0.0_jiti@2.4.2_sass-embedded@1.89.2_terser@5.4_gn7y4dzfmpiwkyxthv3hu732w4/node_modules/@nuxt/node_modules:/mnt/d/website-opd-frontend/node_modules/.pnpm/@nuxt+devtools@2.5.0_vite@6.3.5_@types+node@24.0.0_jiti@2.4.2_sass-embedded@1.89.2_terser@5.4_gn7y4dzfmpiwkyxthv3hu732w4/node_modules:/mnt/d/website-opd-frontend/node_modules/.pnpm/node_modules:$NODE_PATH"
fi
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../@nuxt/devtools/cli.mjs" "$@"
else
  exec node  "$basedir/../@nuxt/devtools/cli.mjs" "$@"
fi
